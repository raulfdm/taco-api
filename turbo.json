{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "db:generate": {
      "outputs": ["dist/generated/**/*"]
    },
    "build": {
      "dependsOn": ["db:generate"],
      "outputs": ["dist/app.js"]
    },
    "dev": {
      "dependsOn": ["db:generate"]
    },
    "lint": {},
    "lint:ci": {},
    "start": {
      "dependsOn": ["build"]
    }
  }
}
