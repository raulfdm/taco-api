  "dependencies": {
    "@prisma/client": "4.13.0",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "express-graphql": "0.12.0",
    "graphql": "16.6.0",
    "graphql-modules": "2.1.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/csvtojson": "2.0.0",
    "@types/express": "4.17.17",
    "@types/node": "18.15.13",
    "csvtojson": "2.0.10",
    "prisma": "4.13.0",
    "tsx": "3.12.6",
    "typescript": "5.0.4"
  },
  "prisma": {
    "seed": "tsx ./src/infrastructure/seed/index.ts"
  }
}
