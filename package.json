{
  "name": "taco-api",
  "version": "1.0.0",
  "description": "Brazilian Table of Food Composition (TACO) - JSON API",
  "type": "module",
  "homepage": "https://github.com/raulfdm/taco.js",
  "bugs": {
    "url": "https://github.com/raulfdm/taco.js/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Raul de Melo",
    "email": "melo.raulf@gmail.com"
  },
  "scripts": {
